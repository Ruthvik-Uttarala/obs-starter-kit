      "type": "timeseries",
      "title": "API Gateway RPS",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{service_name=\"api-gateway\"}[1m]))",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
