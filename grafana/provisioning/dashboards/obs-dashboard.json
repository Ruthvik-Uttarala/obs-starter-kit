    {
      "type": "timeseries",
      "title": "API Gateway Error Rate (5xx)",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{service_name=\"api-gateway\",http_status_code=~\"5..\"}[1m])) / sum(rate(http_server_requests_seconds_count{service_name=\"api-gateway\"}[1m]))",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 }
