    {
      "type": "timeseries",
      "title": "p95 Latency (API Gateway)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_requests_seconds_bucket{service_name=\"api-gateway\"}[1m])))",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 8, "w": 24, "h": 8 }
